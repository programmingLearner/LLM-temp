import matplotlib.pyplot as plt

# Sample data
data1 = [1, 4, 2, 6, 3]
data2 = [3, 5, 1, 7, 4]

# Create a figure and axis
fig, ax = plt.subplots()

# Plot the two datasets
ax.plot(data1, label="Line Chart 1", marker='o')
ax.plot(data2, label="Line Chart 2", marker='o')

# Add titles and labels
ax.set_title("Two Line Charts")
ax.set_xlabel("X-axis Label")
ax.set_ylabel("Y-axis Label")

# Display the legend
ax.legend()

# Show the plot
plt.show()
